{
	"env": {
		"browser": true,
    "node": true,
	},
	"globals": {
		"angular": 1,
		"_": 1,
		"d3": 1,
    "$": 1,
	},
	"rules": {
		"curly": 1,
		"no-mixed-spaces-and-tabs": 1,
		"comma-spacing": 1,
		"comma-dangle": 1,
		"no-trailing-spaces": 1,
		"semi": 1,
		"no-use-before-define": 1,
		"new-parens": 1, 
		"no-underscore-dangle": 1,
		"quotes": 1,
		"no-multi-spaces": 1,
		"key-spacing": 1,
		"no-unused-expressions": 1,
		"new-cap": 1,
		"eqeqeq": 1,
		"no-extra-semi": 1,
	}
}
